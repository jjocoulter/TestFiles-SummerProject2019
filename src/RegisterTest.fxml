<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RegisterTest">
   <children>
      <Label text="Name" AnchorPane.leftAnchor="40.0" AnchorPane.topAnchor="40.0" />
      <Label alignment="CENTER_RIGHT" text="Nickname" AnchorPane.leftAnchor="40.0" AnchorPane.topAnchor="80.0" />
      <Label text="Email" AnchorPane.leftAnchor="40.0" AnchorPane.topAnchor="120.0" />
      <TextField fx:id="tfName" AnchorPane.leftAnchor="100.0" AnchorPane.topAnchor="40.0" />
      <TextField fx:id="tfNick" AnchorPane.leftAnchor="100.0" AnchorPane.topAnchor="80.0" />
      <TextField fx:id="tfEmail" AnchorPane.leftAnchor="100.0" AnchorPane.topAnchor="120.0" />
      <Button fx:id="btnRegister" mnemonicParsing="false" onAction="#doRegister" text="Register" AnchorPane.leftAnchor="120.0" AnchorPane.topAnchor="160.0" />
   </children>
</AnchorPane>
